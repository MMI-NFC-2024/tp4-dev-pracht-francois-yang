---
import Layout from "../layouts/Layout.astro";
---

<Layout>
    <div id="myplot"></div>
</Layout>

<script>
    import * as Plot from "@observablehq/plot";
    import penguin from "../assets/penguins.json";

    const plot = Plot.dot(penguin, {
        x: "culmen_length_mm",
        y: "culmen_depth_mm",
        stroke: "species",
    }).plot();
    const div = document.querySelector("#myplot");
    div?.append(plot);
</script>
