---
import Layout from "../layouts/Layout.astro";
---

<Layout>
    <div id="myplot"></div>
</Layout>

<script>
    import * as Plot from "@observablehq/plot";

    const plot = Plot.rectY(
        { length: 10000 },
        Plot.binX({ y: "count" }, { x: Math.random }),
    ).plot();
    const div = document.querySelector("#myplot");
    div?.append(plot);
</script>
